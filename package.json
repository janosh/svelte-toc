{
  "name": "svelte-toc",
  "description": "Sticky responsive table of contents component written in Svelte",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-toc",
  "repository": "https://github.com/janosh/svelte-toc",
  "license": "MIT",
  "version": "0.5.1",
  "type": "module",
  "main": "./index.js",
  "svelte": "./index.js",
  "bugs": "https://github.com/janosh/svelte-toc/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "test:unit": "vitest tests/unit",
    "test:e2e": "playwright test tests/*.test.ts",
    "test": "vitest tests/unit --run --coverage && playwright test tests/*.test.ts",
    "check": "svelte-check --ignore package",
    "changelog": "npx auto-changelog --package --output changelog.md --hide-credit --commit-limit false --unreleased-only",
    "update-coverage": "vitest tests/unit --run --coverage && npx istanbul-badges-readme"
  },
  "devDependencies": {
    "@playwright/test": "1.29.0",
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@sveltejs/package": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitest/coverage-c8": "^0.26.0",
    "devalue": "^4.2.0",
    "eslint": "^8.30.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "hastscript": "^7.1.0",
    "jsdom": "^20.0.3",
    "mdsvex": "^0.10.6",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "svelte-github-corner": "^0.1.0",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.5.22",
    "typescript": "^4.9.4",
    "vite": "^4.0.2",
    "vitest": "^0.25.8"
  },
  "keywords": [
    "svelte",
    "toc",
    "table of contents",
    "component"
  ],
  "publishConfig": {
    "access": "public"
  }
}
